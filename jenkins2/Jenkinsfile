pipeline {

agent any

environment {
  registry = "https://hub.docker.com/"
  dockercredentials = "docker-cred"  
}

stages {
 stage ('get branch') {
   steps {
		echo 'working branch is ' + "$GIT_BRANCH"
   }
 
 }
 
 stage ('build image') {
 steps {
	echo 'building image'
	script  {
	sh ( script: '''
	    docker images -a
	''')
	
	
	}
 
 }
 }
 
 

}


}
